<?xml version="1.0" encoding="utf-8"?>
<CodeSnippet Format="1.0.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<Header>
		<Title>HttpGet Params</Title>
		<Author>Can Canbolat</Author>
		<Shortcut>getparamsasync</Shortcut>
		<Description>ApiController Http Get Paramaters Endpoint</Description>
		<SnippetTypes>
			<SnippetType>Expansion</SnippetType>
		</SnippetTypes>
	</Header>
	<Snippet>
		<Declarations>
			<Literal Editable="true">
				<ID>param</ID>
				<ToolTip>paramater</ToolTip>
				<Default>param</Default>
				<Function></Function>
			</Literal>
			<Literal Editable="true">
				<ID>EndpointName</ID>
				<ToolTip>Endpoint name</ToolTip>
				<Default>EndpointName</Default>
				<Function></Function>
			</Literal>
		</Declarations>
		<Code Language="csharp" Kind="method decl">
			<![CDATA[[HttpGet("{$param$}")]
			public async Task<IActionResult> $EndpointName$($param$)
			{
				$end$
			
				return Ok();
			}
			]]>
		</Code>
	</Snippet>
</CodeSnippet>